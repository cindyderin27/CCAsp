<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/Css/bootstrap.min.css" rel="stylesheet" />
    <title>@ViewBag.Title</title>
</head>
<body>

    <header class="d-flex flex-column flex-md-row align-items-center p-3 px-md-4 mb-3 bg-body border-bottom shadow-sm">
        <p class="h5 my-0 me-md-auto fw-normal">First Cindy Asp Mvc</p>
        <nav class="my-2 my-md-0 me-md-3">
            @Html.ActionLink("Home", "Index", "Home", null, new { @class = "p-2 text-dark" })
            @Html.ActionLink("About", "About", "Home", new { @class = "p-2 text-dark" })


        </nav>

    </header>


    <div>
        @RenderBody()
    </div>
    <script src="~/Content/Script/jquery-3.5.1.min.js"></script>

</body>
</html>
