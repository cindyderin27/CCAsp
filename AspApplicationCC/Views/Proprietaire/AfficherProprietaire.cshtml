@model IEnumerable<AspApplicationCC.Models.Proprietaire>

@{
    ViewBag.Title = "AfficherProprietaire";
}

<h2>@ViewBag.Title</h2>

<div class="container">
    <form style="border: 1px;">
        <table  border="1" id="cart" class="table table-hover table-condensed">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Nom</th>
                    <th>DateCreation</th>
                   
                </tr>
            </thead>
            <tbody>
                @foreach (var p in Model)
                {
                    <tr>
                        <td data-th="Id">
                            @p.Id
                        </td>

                        <td data-th="Nom">
                            @p.Nom
                        </td>

                        <td data-th="DateCreation">
                            @p.DateCreation
                        </td>

                        
                    </tr>
                }
            </tbody>
        </table>
    </form>
</div>